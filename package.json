{
  "name": "t-music-rpc",
  "version": "1.0.0",
  "description": "T-Music RPC Client with Tray",
  "main": "index.js",
  "type": "module",
  "bin": "index.js",
  "author": "TehPig",
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --out-path=dist"
  },
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "assets": [
      "node_modules/systray2/traybin/**/*"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@performanc/pwsl": "github:PerformanC/Internals#PWSL",
    "@t_bot-team/discord-rpc": "^4.0.5",
    "pkg": "^5.8.1",
    "systray2": "^2.1.4"
  }
}
